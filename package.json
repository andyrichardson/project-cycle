{
  "name": "project-cycle",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/jest": "23.3.0",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.7",
    "@types/react-native": "0.56.3",
    "@types/react-native-permissions": "1.1.1",
    "@types/react-navigation": "2.0.11",
    "@types/react-test-renderer": "16.0.2",
    "babel-preset-react-native": "4.0.0",
    "chai": "4.1.2",
    "jest": "23.4.1",
    "prettier": "1.14.2",
    "prettier-check": "2.0.0",
    "react-dom": "16.5.0",
    "react-native-typescript-transformer": "1.2.10",
    "react-test-renderer": "16.5.0",
    "redux-mock-store": "1.5.3",
    "ts-jest": "23.0.1",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.13.0",
    "typescript": "3.0.3",
    "xmlhttprequest": "1.8.0"
  },
  "scripts": {
    "start": "react-native start --config $(pwd)/rn-cli.config.js",
    "android": "react-native run-android --config $(pwd)/rn-cli.config.js",
    "ios": "react-native run-ios --config $(pwd)/rn-cli.config.js",
    "test": "jest",
    "lint": "tslint --project .",
    "formatting": "prettier-check ./**/*.json ./**/*.tsx ./**/*.md"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native",
    "setupFiles": [
      "./config/setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-native-maps)"
    ]
  },
  "dependencies": {
    "react": "16.5.0",
    "react-native": "0.56.0",
    "react-native-config": "0.11.5",
    "react-native-maps": "0.21.0",
    "react-native-maps-super-cluster": "1.4.1",
    "react-native-percentage-circle": "1.0.7",
    "react-native-permissions": "1.1.1",
    "react-navigation": "2.7.0",
    "react-redux": "5.1.1",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "styled-components": "3.4.5"
  }
}
