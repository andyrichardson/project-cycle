{
  "name": "project-cycle",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/jest": "^22.2.2",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.5",
    "@types/react-native": "^0.55.12",
    "@types/react-navigation": "^1.5.11",
    "@types/react-test-renderer": "^16.0.0",
    "chai": "^4.1.2",
    "jest-expo": "27.0.1",
    "prettier": "^1.12.1",
    "prettier-check": "^2.0.0",
    "react-dom": "^16.3.2",
    "react-native-scripts-ts": "1.13.0",
    "react-native-typescript-transformer": "^1.2.5",
    "react-test-renderer": "16.3.2",
    "ts-jest": "^22.4.2",
    "tslib": "^1.9.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.1",
    "xmlhttprequest": "^1.8.0"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native run-android --config $(pwd)/rn-cli.config.js",
    "ios": "react-native-scripts-ts ios",
    "test": "jest",
    "lint": "tslint --project .",
    "formatting": "prettier-check ./**/*.json ./**/*.tsx ./**/*.md",
    "postinstall": "config/set-target.sh"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "setupFiles": [
      "./config/setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "expo": "^27.0.1",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "react": "16.3.2",
    "react-native": "0.55.4",
    "react-native-maps": "^0.21.0",
    "react-navigation": "^2.0.1",
    "styled-components": "^3.2.6"
  }
}
